@phdthesis{Stedman:1999,
	Abstract = {The many-fermion problem is one of the most fundamental and intractable in condensed 
matter physics. Quantum Monte Carlo methods are almost the only practical way to 
calculate properties of many-body systems without resorting to `mean-field' approxima- 
tions. In this thesis we investigate the widely-used ground-state methods of Variational 
Monte Carlo (vmc) and Diffusion Monte Carlo (dmc) together with the Path Integral 
Monte Carlo (pimc) method for boson systems at finite temperature and its proposed 
fermion equivalent, Restricted pimc (rpimc). 
We introduce a stochastic method to increase the sampling efficiency of a general 
Monte Carlo method and apply it to the vmc simulation of full-core atoms. We achieve 
an increase in efficiency of at least a factor of four compared with standard vmc. Further, 
we find an approximate analytic fit to our results which gives a comparable increase in 
efficiency at essentially no cost. In addition, we develop the Talus modelling suite to 
provide an extensible environment for the study of new algorithms and systems. 
At finite temperature, pimc has been used with great success, however, rpimc has 
not been much developed or tested. We prove that the rpimc algorithm obeys a varia- 
tional principle for the free energy and develop a new modeller to test its performance 
in simple systems. We find that the primitive algorithm is slow and suffers from severe 
time-step errors at low temperature. 
Further, sampling becomes very inefficient as the temperature is lowered because 
the random walk becomes trapped in a small region of phase space. In an attempt 
to overcome this problem, we introduce a new importance-sampling transformation for 
rpimc which gives a generalized Feynman-Kac formula for the fermionic density matrix. 
This leads us to propose a new drift-diffusion sampling scheme for rpimc which is closer 
in spirit to dmc. 
},
	Author = {Stedman, Mark Laurence},
	Date-Added = {2009-06-29 18:26:40 +1000},
	Date-Modified = {2009-06-29 18:29:49 +1000},
	Month = {July},
	School = {University of London},
	Title = {{Ground-state and finite-temperature quantum Monte Carlo simulations of many-particle systems}},
	Type = {PhD Thesis},
	Year = {1999},
	Bdsk-File-1 = {}}

@unpublished{Polkovnikov:2009,
	Abstract = {We formulate representation of quantum dynamics of many-particle interacting systems through classical trajectories as a perturbative expansion in quantum fluctuations around the classical limit. We explicitly consider three different classical limits in the coordinate-momentum representation (corpuscular classical limit), coherent state representation (wave classical limit), and the classical limit for spin systems. We describe how one recovers truncated Wigner approximation (TWA) in the leading order in quantum fluctuations around each of these limits. Further quantum corrections can be found either in the form of non-linear response of observables to infinitesimal quantum jumps or equivalently in terms of stochastic quantum jumps with non-positive weight. We discuss how one can find both equal and non-equal time correlation functions within this approach and which correlation functions have casual representation. Our approach is closely related to phase space methods based on Weyl quantization, coherent state phase space methods, and Keldysh diagrammatic technique. We show how both new results and earlier concepts naturally emerge from combining path-integral approach and the Heisenberg representation. We illustrate this approach with various examples including sine-Gordon model, one- and two-dimensional Bose Hubbard model, Dicke model and others.},
	Author = {Polkovnikov, Anatoli},
	Date-Added = {2009-06-29 17:25:00 +1000},
	Date-Modified = {2009-07-07 16:53:07 +1000},
	Eprint = {arXiv:0905.3384},
	Month = {May},
	Title = {Representation of quantum dynamics of interacting systems through classical trajectories},
	Year = {2009},
	Bdsk-File-1 = {}}

