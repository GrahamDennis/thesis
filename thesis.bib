@article{Haine:2003fs,
	Abstract = {A semiclassical, multimode model of a continuously pumped atom laser is presented. For a spatially independent coupling process it is found that the system is unstable below a critical scattering length. As large atomic interactions will increase the phase diffusion of the lasing mode, it is desirable to obtain a stable atom laser with low nonlinearity. It is shown that spatially dependent pumping stabilizes the atom laser to a finite number of modes, and can induce single-mode operation.},
	Author = {{Haine}, S.~A. and {Hope}, J.~J.},
