@article{Marion:2003,
	Abstract = {Over five years, we have compared the hyperfine frequencies of 133Cs and 87Rb atoms in their electronic ground state using several laser-cooled 133Cs and 87Rb atomic fountains with an accuracy of ∼10-15. These measurements set a stringent upper bound to a possible fractional time variation of the ratio between the two frequencies: d/dtln⁡[(νRb)/(νCs)]=(0.2$\pm$7.0){\times}10-16   yr-1 (1σ uncertainty). The same limit applies to a possible variation of the quantity (μRb/μCs)α-0.44, which involves the ratio of nuclear magnetic moments and the fine structure constant.
},
