	Date-Modified = {2009-11-11 14:20:40 +1100},
	Journal = {J. Phys. A},
	Number = {36},
	Pages = {365301 (16pp)},
	Title = {Regularized tripartite continuous variable EPR-type states with Wigner functions and CHSH violations},
	Url = {http://stacks.iop.org/1751-8121/41/365301},
	Volume = {41},
	Year = {2008},
