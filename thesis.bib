	Fullpages = {3540--3552},
	Ignoretitle = {Turbulent relaxation of compressible plasmas with flow},
	Journal = {Physics of Fluids},
	Keywords = {MHD, Relaxation-energy, Relaxation-entropy},
	Pages = {3540},
	Publisher = {AIP},
	Title = {Turbulent relaxation of compressible plasmas with flow},
	Volume = {26},
	Year = {1983},
