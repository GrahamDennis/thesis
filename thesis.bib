	Number = {5A},
	Pages = {A393},
	Title = {Self-organization and its effect on confinement in a reversed field pinch plasma},
	Url = {http://stacks.iop.org/0741-3335/39/i=5A/a=036},
	Volume = {39},
	Year = {1997},
