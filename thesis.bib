	Month = {Mar},
	Numpages = {14},
	Pages = {033613},
	Publisher = {American Physical Society},
	Title = {Multiconfigurational time-dependent Hartree method for bosons: Many-body dynamics of bosonic systems},
	Url = {http://link.aps.org/doi/10.1103/PhysRevA.77.033613},
	Volume = {77},
	Year = {2008},
