	Number = {11},
	Pages = {3960-3973},
	Publisher = {AIP},
	Title = {Cusp and Y-type magnetic structures and velocity fields at the endpoint of the reconnection layer},
	Url = {http://link.aip.org/link/?PHP/4/3960/1},
	Volume = {4},
	Year = {1997},
	Bdsk-File-1 = {},
