	Journal = {Genes & Development},
	Number = {21},
	Pages = {2551-2562},
	Title = {Enzymatic regulation of pattern: BMP4 binds CUB domains of Tolloids and inhibits proteinase activity},
	Url = {http://genesdev.cshlp.org/content/23/21/2551.abstract},
	Volume = {23},
	Year = {2009},
