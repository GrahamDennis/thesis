	Journal = {Plasma Physics and Controlled Fusion},
	Number = {8},
	Pages = {084001},
	Title = {Magnetohydrodynamic equilibrium and the stability of tokamak and reversed-field pinch systems with 3D helical cores},
	Url = {http://stacks.iop.org/0741-3335/53/i=8/a=084001},
	Volume = {53},
	Year = {2011},
