	Day = {21},
	Isbn = {0028-0836},
	Journal = {Nature},
	M3 = {10.1038/nature05094},
	Month = {September},
	Number = {7109},
	Pages = {312--315},
	Title = {Spontaneous symmetry breaking in a quenched ferromagnetic spinor Bose-Einstein condensate},
	Ty = {JOUR},
